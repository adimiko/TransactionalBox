{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "TransactionalBox.*": "Information"
      }
    }
  },

  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },

  "AllowedHosts": "*"
}
